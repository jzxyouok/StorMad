	background-color: #fff;
	font-size: 0;
	border-radius: 15px;
	transition: .1s linear;
	box-sizing: border-box!important
}

.industry-type .layui-form-checkbox:hover {
	border: none;
}

.industry-type .layui-form-checkbox span {
	padding: 0 20px;
	font-size: 12px;
	line-height: 30px;
	background-color: #d2d2d2;
	color: #333;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	border-radius: 15px;
}

.industry-type .layui-form-checkbox:hover span {
	background-color: #c2c2c2;
}

.industry-type .layui-form-checkbox i {
	display: none;
	position: absolute;
	right: 0;
	width: 37px;
	color: #fff;
	font-size: 20px
}

.industry-type .layui-form-checkbox:hover i {
	color: #2089ff
}

.industry-type .layui-form-checked,
.industry-type .layui-form-checked:hover {
	border: none;
}

.industry-type .layui-form-checked span,
.industry-type .layui-form-checked:hover span {
	color: #fff;
	background-color: #2089ff
}

.industry-type .layui-form-checked i,
.industry-type .layui-form-checked:hover i {
	color: #2089ff;
}


/*industry-type checkbox end*/

.creat-schedule .layui-form-item .layui-input-inline span {
	line-height: 30px;
}

.creat-schedule .layui-form-item .layui-input-inline:last-child {
	margin-left: 0px;
}

.layui-form-select dl dd.layui-this {
	background-color: #2089ff;
	color: #fff;
}


/*radio*/

.layui-form-radio {
	line-height: 30px;
	margin: 0px 10px 0 0;
	padding-right: 10px;
	cursor: pointer;
	font-size: 0;
}

.layui-form-radio i {
	margin-right: 8px;
	font-size: 23px;
	color: #e3e3e3;
}

.layui-form-radio span {
	font-size: 12px;
}

.layui-form-radio i:hover,
.layui-form-radioed i {
	color: #2089ff;
}


/*radio end*/

.creat-schedule .layui-form-item .choice-time {
	display: inline-block;
	/*line-height: 30px;*/
	/*padding-left: 15px;*/
}


