/*locational-choices*/

.locational-choices .choice-box {
	font-size: 12px;
	height: 300px;
	border: 1px solid #e3e3e3;
	padding: 0;
	white-space: inherit;
}

.choice-box .table> thead> tr> th {
	color: #fff;
	background-color: #2089ff;
	height: 28px;
	line-height: 28px;
}


/*checkbox*/

.locational-choices .table .check-box {
	display: inline-block;
	margin: 0;
}

.locational-choices .table .check-box .layui-form-checkbox {
	display: none;
}

.creat-schedule .locational-choices .choice-check .table .check-box span.check-box {
	width: 20px;
	height: 20px;
	min-height: auto;
	line-height: 20px;
	display: inline-block;
	position: relative;
	vertical-align: middle;
}

.creat-schedule .locational-choices .choice-check .table .check-box span.check-box input {
	display: inherit;
	filter: alpha(opacity=0);
	-moz-opacity: 0;
	-khtml-opacity: 0;
	-webkit-opacity: 0;
	opacity: 0;
	width: 20px;
	height: 20px;
	position: relative;
	z-index: 9;
	margin: 0;
}

.creat-schedule .locational-choices .choice-check .table .check-box span.check-box i {
	width: 20px;
	height: 20px;
	border: 1px solid #2089ff;
	border-radius: 0px;
	display: inline-block;
	position: absolute;
	left: 0;
	top: 0;
}

.creat-schedule .locational-choices .choice-check .table .check-box span.check-box i {
	border-radius: 0px;
}

.creat-schedule .locational-choices .choice-box .table .check-box span.check-box input:checked+ i {
	background-color: #2089ff;
	border-color: #2089ff;
}

.creat-schedule .locational-choices .choice-check .table .check-box span.check-box input:checked+ i:after {
	content: "";
	width: 8px;
	height: 5px;
	display: inline-block;
	position: absolute;
	left: 50%;
	top: 50%;
	border: 2px solid #fff;
	margin-left: -4px;
	margin-top: -4px;
	border-top: 0;
	border-right: 0;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	transform: rotate(-45deg);
}


/*checkbox end*/


