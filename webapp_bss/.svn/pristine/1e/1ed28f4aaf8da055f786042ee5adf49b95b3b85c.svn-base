.wrapper {
	margin: 0;
	padding: 0;
}


/*header*/

header {
	position: fixed;
	width: 100%;
	height: 80px;
	background-color: #fff;
	box-shadow: 0px 4px 5px rgba(0, 0, 0, .3);
	z-index: 9999;
}

header .logo {
	width: 260px;
	height: 80px;
	line-height: 80px;
	background-color: #003070;
	padding-left: 12px;
}

header .pull-right {
	position: absolute;
	top: 0;
	right: 0;
	line-height: 80px;
}

header .pull-right> p {
	display: inline-block;
	padding-right: 30px;
	/*line-height: 80px;*/
}

header .pull-right> a {
	position: relative;
	display: inline-block;
	color: #fff;
	height: 80px;
	padding: 0 20px;
	background-color: #2089ff;
	text-decoration: none;
}

header .pull-right> a> i {
	position: absolute;
	top: 14px;
	left: 34px;
	width: 28px;
	height: 28px;
}

header .pull-right> a> i.log-out-icon {
	background: url(../img/css-sprite.png) -150px 0 no-repeat;
}

header .pull-right> a> span {
	position: relative;
	top: 18px;
	left: 0px;
}


/*header end*/

.wrapper .col-lg-12 {
	padding-left: 275px;
	margin-top: 95px;
	margin-bottom: 120px;
	/*overflow: hidden;*/
}


/*top*/

.mainBody .top {
	margin-bottom: 10px;
	background-color: #fff;
	box-shadow: 0px 1px 2px rgba(0, 0, 0, .3);
}

.mainBody .top .title {
	height: 55px;
	border-bottom: 1px solid #e9e9e9;
	padding-right: 20px;
}

.mainBody .top .title h2 {
	display: inline-block;
	color: #2089ff;
	font-size: 16px;
	line-height: 18px;
	padding-left: 18px;
	margin: 18px 0;
	border-left: 4px solid #2089ff;
}

.mainBody .top .title p {
	display: inline-block;
	color: #252525;
	font-size: 12px;
	line-height: 55px;
}

.mainBody .top .title p .fa {
	color: #7d7d7d;
	font-size: 16px;
	padding-right: 7px;
}

.mainBody .top .search-box {
	height: 70px;
	padding: 20px;
}

.search-box .layui-form-label {
	float: left;
	display: block;
	width: auto;
	text-align: right;
	line-height: 30px;
	font-weight: normal;
	padding: 0;
	margin: 0;
}

.search-box .layui-form-label {
	margin-left: 30px;
}

.search-box .layui-form-label:first-child {
	margin-left: 0;
}

.search-box .layui-form-item .layui-input-inline {
	width: auto;
}

.search-box .layui-form-item .layui-input-inline:last-child {
	margin-left: 30px;
}

.search-box .layui-input {
	height: 30px;
	line-height: 30px;
}

.search-box .layui-form-select ul li.layui-this {
	background-color: #2089ff;
	color: #fff;
}

.search-box .layui-btn {
	height: 30px;
	line-height: 30px;
	background-color: #2089ff;
	padding: 0 28px;
}


/*top end*/


/*content*/

.mainBody .content {
	padding: 10px 20px;
	background-color: #fff;
	box-shadow: 0px 1px 2px rgba(0, 0, 0, .3);
}

.content .table> thead> tr {
	background-color: #eee;
}

.content .table> thead> tr> th {
	padding: 0 8px;
	line-height: 42px;
	border-bottom: none;
}

.content .table> tbody> tr> td {
	padding: 0 8px;
	line-height: 42px;
	vertical-align: middle;
	border-top: 1px solid #f0f0f0;
	background-color: #fff;
}

.content .table> tbody> tr.active> td,
.content .table> tbody> tr.active:hover> td,
.content .table> tbody> tr:hover> td {
	background-color: #e9f2ff;
}

.content .table> tbody> tr:last-child {
	border-bottom: 1px solid #f0f0f0;
}

.content .table> tbody> tr> td> p {
	position: relative;
}

.content .table> tbody> tr> td> p> span {
	padding-right: 8px;
}

.content .table> tbody> tr> td> p> i {
	position: absolute;
	top: 14px;
	/*left: 5px;*/
	width: 23px;
	height: 14px;
}

.content .table> tbody> tr> td> p> i.ad-preview-icon {
	background: url(../img/css-sprite.png) -120px 0 no-repeat;
}


/*td a*/

.content .table> thead> tr> th:last-child {
	text-align: center;
}

.content .table> tbody> tr> td> a {
	color: #2089ff;
	text-decoration: underline;
}

.content .table> tbody> tr> td:last-child {
	text-align: center;
}

.content .table> tbody> tr> td:last-child> a {
	color: #2089ff;
	text-decoration: none;
	margin: 0 20px;
}

.ad-management-list .content .table> tbody> tr> td:last-child,
.schedule-management .content .table> tbody> tr> td:last-child {
	color: #d7d7d7;
}


/*td a end*/

.content .page-number {
	text-align: right;
}


/*schedule-management*/

.schedule-management .search-box .layui-btn:last-child {
	height: 30px;
	line-height: 30px;
	text-decoration: none;
	background-color: #00cb43;
	padding: 0 28px;
}


/*schedule-management end*/


/*creat-schedule*/

.creat-schedule .content {
	font-size: 12px;
	padding: 30px 120px;
}

.creat-schedule .layui-input {
	display: inline-block;
	max-width: 290px;
	height: 30px;
	line-height: 30px;
	border: 1px solid #e1e1e1;
	border-radius: 0px;
}

.creat-schedule .layui-form-label {
	float: left;
	display: inline-block;
	padding: 0;
	width: 120px;
	text-align: right;
	line-height: 30px;
	font-weight: normal;
}

.creat-schedule .layui-form-label span {
	color: #ff0000;
	padding-right: 8px;
}

.creat-schedule .layui-form-item .layui-input-inline {
	width: auto;
	margin-left: 10px;
}

.creat-schedule .layui-form-item .layui-input-inline:last-child {
	/*margin-left: 30px;*/
}

.creat-schedule .layui-form-item .layui-input-block {
	/*display: inline-block;*/
