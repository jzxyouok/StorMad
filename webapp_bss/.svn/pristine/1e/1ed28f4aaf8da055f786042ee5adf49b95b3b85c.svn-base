	min-height: 30px;
}

.creat-schedule .layui-form-item .layui-input-block span.note {
	display: inline-block;
	line-height: 30px;
	padding-left: 15px;
}

.industry-type .layui-form-checkbox {
	position: relative;
	height: 30px;
	line-height: 28px;
	margin: 0 10px 10px 0;
	padding-right: 0px;
