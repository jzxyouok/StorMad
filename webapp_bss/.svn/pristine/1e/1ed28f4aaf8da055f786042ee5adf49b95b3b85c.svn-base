/*checkbox*/

.ad-area-list .content .table .checkbox {
	display: inline-block;
	margin: 0;
}

.ad-area-list .checkbox span.checkbox {
	width: 20px;
	height: 20px;
	min-height: auto;
	line-height: 20px;
	display: inline-block;
	position: relative;
	vertical-align: middle;
}

.ad-area-list .checkbox span.checkbox input {
	filter: alpha(opacity=0);
	-moz-opacity: 0;
	-khtml-opacity: 0;
	-webkit-opacity: 0;
	opacity: 0;
	width: 20px;
	height: 20px;
	position: relative;
	z-index: 9;
	margin: 0;
}

.ad-area-list .checkbox span.checkbox i {
	width: 20px;
	height: 20px;
	border: 1px solid #2089ff;
	border-radius: 0px;
	display: inline-block;
	position: absolute;
	left: 0;
	top: 0;
}

.ad-area-list .checkbox span.checkbox i {
	border-radius: 0px;
}

.ad-area-list .checkbox span.checkbox input:checked+ i {
	background-color: #2089ff;
	border-color: #2089ff;
}

.ad-area-list .checkbox span.checkbox input:checked+ i:after {
	content: "";
	width: 8px;
	height: 5px;
	display: inline-block;
	position: absolute;
	left: 50%;
	top: 50%;
	border: 2px solid #fff;
	margin-left: -4px;
	margin-top: -4px;
	border-top: 0;
	border-right: 0;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	transform: rotate(-45deg);
}


