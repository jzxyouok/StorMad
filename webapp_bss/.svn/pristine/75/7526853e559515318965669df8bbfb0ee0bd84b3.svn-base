 /* 页面全局样式 global start */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* 全局 body/html/wrapper/container start */
 
 @-webkit-viewport {
     width: device-width;
 }
 
 @-moz-viewport {
     width: device-width;
 }
 
 @-ms-viewport {
     width: device-width;
 }
 
 @-o-viewport {
     width: device-width;
 }
 
 @viewport {
     width: device-width;
 }
 
 body {
     /*min-width: 1260px;*/
     background-color: #e9e9e9;
 }
 
 a {
     color: #2089ff;
     text-decoration: none;
     /*padding: 0 5px;*/
 }
 
 a:focus,
 a:hover {
     color: #3292fe;
     text-decoration: underline;
     /*padding: 0 5px;*/
 }
 
 .wrapper {
     margin: 0;
     padding: 0;
 }
 
 .container {
     width: 1400px;
 }
 
 .container-fluid {
     min-width: 1280px;
 }
 
 .active-status {
     color: #c0c0c0 !important;
     cursor: not-allowed;
 }
 
 .disabled {
     color: #c0c0c0 !important;
     cursor: not-allowed;
 }
 
 .fa {
     padding-right: 5px;
 }
 /* 全局 body/html/wrapper/container end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* 自定义图标 icon start */
 
 p.tips {
     position: relative;
     color: #f56760;
     font-size: 12px;
     float: right;
     padding-left: 20px;
     margin-bottom: 0;
 }
 
 p i {
     width: 14px;
     height: 14px;
     text-align: center;
     position: absolute;
     /*top: 0;
     left: 0;*/
 }
 
 p i.exclamation-icon {
     background: url(../img/css-sprite.png) -180px 0 no-repeat;
 }
 
 .empty-tips {
     width: auto;
     margin-top: 80px;
     margin-bottom: 80px;
     text-align: center;
 }
 
 .empty-tips p {
     display: block;
     position: relative;
     width: 220px;
     font-size: 16px;
     line-height: 50px;
     left: 45%;
 }
 
 .empty-tips p i {
     position: absolute;
     left: 0;
     top: 18px;
 }
 
 .empty-tips p span {
     display: block;
     padding-left: 25px;
     text-align: left;
 }
 /* 自定义图标 icon end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* 按钮 button start */
 /*默认显示*/
 
 .btn-default {
     color: #fff;
     text-shadow: none;
     width: 100%;
     /*height: 50px;*/
     background-color: #2089ff;
     border: 1px solid #2089ff;
     border-radius: 2px;
     outline: 0;
 }
 
 .btn-default:hover,
 .btn-default:focus {
     color: #fff;
     text-shadow: none;
     width: 100%;
     height: 50px;
     background-color: #3292fe;
     border: 1px solid #2089ff;
     outline: 0;
 }
 /*按下后状态*/
 
 .btn-default:active:focus {
     color: #fff;
     background-color: #3292fe;
     border-color: transparent;
     border-top: 5px solid #2089ff;
     box-shadow: none;
     outline: 0;
 }
 /* 按钮 button end */
 /* 按钮组 button-box start */
 
 .button-box {
     text-align: center;
 }
 
 .button-box button {
     height: 30px;
     line-height: 30px;
 }
 
 .button-box .layui-btn,
 .button-box .layui-btn:focus {
     color: #fff;
     background-color: #2089ff;
     text-decoration: none;
 }
 
 .button-box .layui-btn-primary,
 .button-box .layui-btn-primary:focus {
     color: #2089ff;
     background-color: transparent;
     border: 1px solid #2089ff;
 }
 /* 按钮组 button-box end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* 表单 form start */
 /* 表单 form end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* 表格 table start */
 
 .table> thead> tr {
     background-color: #eee;
 }
 
 .table> thead> tr> th {
     padding: 0 15px;
     line-height: 42px;
     border-bottom: none;
     white-space: nowrap;
 }
 
 .table> tbody> tr> td {
     padding: 0 15px;
     line-height: 42px;
     vertical-align: middle;
     border-top: 1px solid #f0f0f0;
     background-color: #fff;
     white-space: nowrap;
 }
 
 .table> tbody> tr.active> td,
 .table> tbody> tr.active:hover> td,
 .table> tbody> tr:hover> td {
     background-color: #e9f2ff;
 }
 
 .table> tbody> tr:last-child {
     border-bottom: 1px solid #f0f0f0;
 }
 /*td a*/
 
 .content .table> thead> tr> th:last-child,
 .content .table> tbody> tr> td:last-child {
     text-align: center;
 }
 
 .content .table> tbody> tr> td:last-child> a {
     /*color: #2089ff;
     text-decoration: none;*/
     margin: 0 20px;
 }
 /*td a end*/
 /* 表格 table end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /*layui style*/
 
 .layui-laypage .layui-laypage-curr .layui-laypage-em {
     background-color: #2089ff;
 }
 /*search*/
 
 .layui-form-item .layui-form-label {
     float: left;
     display: block;
     width: auto;
     text-align: right;
     line-height: 30px;
     font-weight: normal;
     padding: 0;
     margin: 0;
 }
 
 .layui-form-item .layui-form-label {
     margin-left: 20px;
 }
 
 .layui-form-item .layui-input-inline {
     width: auto;
 }
 
 .layui-form-item .layui-input-inline span {
     line-height: 30px;
 }
 
 .layui-form-item .layui-input-inline .layui-input {
     height: 30px;
     line-height: 30px;
 }
 
 .layui-form-select dl {
     top: 32px;
 }
 
 .layui-form-select dl dd.layui-this {
     background-color: #2089ff;
     color: #fff;
 }
 
 .layui-btn {
     height: 30px;
     line-height: 30px;
     background-color: #2089ff;
     padding: 0 28px;
 }
 
 .layui-btn:focus {
     color: #fff;
 }
 /*search end*/
 /*radio start*/
 
 .layui-form-radio {
     line-height: 30px;
     margin: 0px 10px 0 0;
     padding-right: 10px;
     cursor: pointer;
     font-size: 0;
 }
 
 .layui-form-radio i:hover,
 .layui-form-radioed i {
     color: #2089ff;
 }
 /*radio end*/
 /*laypage start*/
 
 .layui-laypage {
     margin: 0;
 }
 
 .layui-laypage a {
     text-decoration: none;
 }
 /*laypage end*/
 /*layer start*/
 
 .layui-layer-page .layui-layer-content {
     height: auto !important;
 }
 /*layer end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /* header start */
 
 header {
     position: fixed;
     width: 100%;
     height: 80px;
     background-color: #fff;
     box-shadow: 0px 4px 5px rgba(0, 0, 0, .3);
     z-index: 9999;
 }
 
 header .logo {
     width: 260px;
     height: 80px;
     line-height: 80px;
     background-color: #003070;
     padding-left: 12px;
 }
 
 header .pull-right {
     position: absolute;
     top: 0;
     right: 0;
     line-height: 80px;
 }
 
 header .pull-right> p {
     display: inline-block;
     padding-right: 30px;
     /*line-height: 80px;*/
 }
 
 header .pull-right> a {
     position: relative;
     display: inline-block;
     color: #fff;
     height: 80px;
     padding: 0 20px;
     background-color: #2089ff;
     text-decoration: none;
 }
 
 header .pull-right> a> i {
     position: absolute;
     top: 14px;
     left: 34px;
     width: 28px;
     height: 28px;
 }
 
 header .pull-right> a> i.log-out-icon {
     background: url(../img/css-sprite.png) -150px 0 no-repeat;
 }
 
 header .pull-right> a> span {
     position: relative;
     top: 18px;
     left: 0px;
 }
 /* header end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* --------------------------------------------------
-------------------------------------------------- */
 /* main-body start */
 
 .wrapper .main-body {
     padding-left: 275px;
     margin-top: 95px;
     margin-bottom: 50px;
     min-width: 990px;
     min-height: calc(100vh - 95px - 50px - 64px);
 }
 /* --------------------------------------------------
-------------------------------------------------- */
 /*top*/
 
 .main-body .top {
     margin-bottom: 10px;
     background-color: #fff;
     box-shadow: 0px 1px 2px rgba(0, 0, 0, .3);
 }
 /* --------------------------------------------------
-------------------------------------------------- */
 /*title*/
 
 .main-body .top .title {
     height: 55px;
     border-bottom: 1px solid #e9e9e9;
     padding-right: 20px;
 }
 
 .main-body .top .title h2 {
     display: inline-block;
     color: #2089ff;
     font-size: 16px;
     line-height: 18px;
     padding-left: 18px;
     margin: 18px 0;
     border-left: 4px solid #2089ff;
 }
 
 .main-body .top .title p {
     display: inline-block;
     color: #252525;
     font-size: 12px;
     line-height: 55px;
 }
 
 .main-body .top .title p .fa {
     color: #7d7d7d;
     font-size: 16px;
     padding-right: 7px;
     position: inherit;
 }
 
 .main-body .top form {
     min-width: 990px;
     height: 70px;
     padding: 20px;
 }
 
 .main-body .top form .layui-form-item .layui-form-label:first-child {
     margin-left: 0;
 }
 
 .main-body .top form .layui-form-item .layui-input-inline:last-child {
     margin-left: 30px;
 }
 /*title end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*content*/
 
 .main-body .content {
     padding: 10px 20px;
     padding-bottom: 30px;
     background-color: #fff;
     box-shadow: 0px 1px 2px rgba(0, 0, 0, .3);
 }
