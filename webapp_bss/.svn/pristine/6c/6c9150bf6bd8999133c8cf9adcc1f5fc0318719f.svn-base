/*addSpecification*/

.layui-layer-content .add-specification .layui-form .layui-form-label {
    margin-left: 0;
}

.layui-layer-content .add-specification .layui-form .layui-input-block {
    margin-left: 80px;
}

.layui-layer-content .add-specification .layui-form .layui-input-block .layui-input {
    min-width: 300px;
    height: 30px;
    line-height: 30px;
}

.layui-layer-content .add-specification .type {
    padding: 15px 10px;
    border: 1px dashed #cbcbcb;
}

.layui-layer-content .add-specification .type .layui-form-item:last-child {
    margin-bottom: 0;
}

.layui-layer-content .add-specification .type .layui-form-label {
    width: 120px;
    text-align: right;
}

.layui-layer-content .add-specification .type .layui-input-block {
    margin-left: 130px;
}


/*addSpecification end*/


/*invoicePreview*/

.layui-layer-content .invoice-preview p {
    color: #949494;
    font-size: 12px;
}

.layui-layer-content .invoice-preview p span {
    color: #000;
    font-size: 12px;
    line-height: 24px;
}

.layui-layer-content .invoice-info-review p span:first-child {
    display: inline-block;
    color: #949494;
    width: 70px;
    text-align: right;
}

.layui-layer-content .invoice-info-review .button-box {
    padding-top: 20px;
    padding-bottom: 10px;
}

.layui-layer-content .invoice-info-review .button-box a {
    color: #2089ff;
    line-height: 30px;
    text-decoration: underline;
    margin-left: 10px;
}


/*invoicePreview end*/

/*administrators-window*/

.layui-layer-content .administrators-window .layui-form .layui-form-label {
    width: 90px;
    margin-right: 10px;
}

.layui-layer-content .administrators-window .layui-form .layui-input-inline {
    min-width: 200px;
}


/*administrators-window end*/
