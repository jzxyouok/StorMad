<!DOCTYPE html>
<html lang="zh-CN">

<head>
    <title>永辉竞价投放后台系统 - 用户登录</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="renderer" content="webkit">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="keywords" content="keywords" />
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="favicon.ico">
    <!-- Bootstrap core CSS -->
    <link href="../extend/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="../assets/css/ie10-viewport-bug-workaround.css" rel="stylesheet">
    <!-- Just for debugging purposes. Don't actually copy these 2 lines! -->
    <!--[if lt IE 9]><script src="../assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <script src="../assets/js/ie-emulation-modes-warning.js"></script>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--extend css-->
    <link rel="stylesheet" href="../extend/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="../extend/layui/css/layui.css">
    <!-- Custom styles for this non-responsive system -->
    <!-- <link href="extend/bootstrap/css/non-responsive.css" rel="stylesheet"> -->
    <!-- Custom css -->
    <link href="../css/bss-global.css" rel="stylesheet">
    <link href="../css/bss-login.css" rel="stylesheet">
    <link href="../css/bss-responsive.css" rel="stylesheet">
</head>

<body>
    <!-- header start -->
    <header class="login-header">
        <div class="container">
            <div class="logo">
                <a href="#"><img src="../img/login-logo.png" /></a>
            </div>
        </div>
    </header>
    <!-- /.header start -->
    <!-- wrapper -->
    <div class="wrapper">
        <!-- wrapper-inner -->
        <div class="wrapper-inner">
            <!-- login-container -->
            <!-- <div class="wrapper-container login-container"> -->
            <div class="container login-container">
                <div class="row">
                    <div class="left">
                        <img src="../img/login-img.png" />
                    </div>
                    <div class="right">
                        <form class="layui-form form-horizontal form-login">
                            <div class="form-group">
                                <i class="input-icon user-icon"></i>
                                <input type="text" class="form-control" maxlength="10" lay-verify="user" name="user" placeholder="输入用户名">
                            </div>
                            <div class="form-group">
                                <i class="input-icon password-icon"></i>
                                <input type="password" class="form-control" maxlength="10" lay-verify="password" name="password" placeholder="输入密码">
                            </div>
                            <div class="form-group">
                                <div class="verification-code">
                                    <i class="input-icon verification-icon"></i>
                                    <input type="text" class="form-control" lay-verify="code" maxlength="5" id="code" name="code" placeholder="输入验证码">
                                    <input type="hidden" id="vid" name="vid" value="">
                                </div>
                                <div class="verification-code-pic">
                                    <a href="javascript:void(0)"><img id="code-pic" src="http://placehold.it/125x50"></a>
                                </div>
                            </div>
                            <div class="form-group button-box">
                                <button type="button" lay-submit lay-filter="sub" class="btn btn-default">登录</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- /.login-container -->
        </div>
        <!-- /.wrapper-inner -->
    </div>
    <!-- /.wrapper -->
    <!--footer-->
    <footer>
        <div class="container">
            <p>Copyright @2016
                <a href="#">永辉超市股份有限公司</a> All Rights Reserved.</p>
        </div>
    </footer>
    <!--footer-->
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="../js/jquery.min.js"></script>
    <script>
    window.jQuery || document.write('<script src="../assets/js/vendor/jquery.min.js"><\/script>')
    </script>
    <script src="../extend/bootstrap/js/bootstrap.min.js"></script>
    <!-- Just to make our placeholder images work. Don't actually copy the next line! -->
    <script src="../assets/js/vendor/holder.min.js"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="../assets/js/ie10-viewport-bug-workaround.js"></script>
    <!-- Extend JavaScript -->
    <script src="../extend/layui/layui.js"></script>
    <!-- respond JavaScript -->
    <script src="../js/respond.src.js"></script>
    <!-- Custom JavaScript -->
    <!-- <script src="js/bss-custom.js"></script>
    <script src="js/common.js"></script>
    <script src="ader/js/login.js"></script> -->
    <script src="../js/bss-custom.js"></script>
    <script src="../js/common.js"></script>
    <script src="js/login.js"></script>
</body>

</html>
