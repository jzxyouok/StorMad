 /*shippingAddressEdit*/
 
 .layui-layer-content .shipping-address-edit .layui-form-label {
     color: #666;
     font-size: 12px;
     line-height: 40px;
     width: auto;
     padding: 0 10px;
 }
 
 .layui-layer-content .shipping-address-edit .layui-input-inline {
     width: auto;
 }
 
 .layui-layer-content .shipping-address-edit .district-wrap {
     margin-right: 0;
 }
 
 .layui-layer-content .shipping-address-edit .layui-input {
     height: 40px;
     line-height: 40px;
     margin-bottom: 10px;
 }
 
 .layui-layer-content .shipping-address-edit .layui-form-select dl {
     max-height: 255px !important;
 }
 
 .layui-layer-content .shipping-address-edit .button-box .btn-default:hover,
 .layui-layer-content .shipping-address-edit .button-box .btn-default:focus {
     color: #fff;
     text-shadow: none;
     background-color: #f5715f;
     border: 1px solid #c74545;
     outline: 0;
 }
 
 .layui-layer-content .shipping-address-edit .button-box .btn-default:active:focus {
     color: #fff;
     background-color: #f5715f;
     border-color: transparent;
     border-top: 5px solid #c74545;
     box-shadow: none;
     outline: 0;
 }
 /*shippingAddressEdit end*/
