 /* --------------------------------------------------
-------------------------------------------------- */
 /* global style start */
 
 .layui-layer-page .layui-layer-content {
     padding: 20px;
     overflow: inherit !important;
 }
 
 .layui-layer-content .layui-form .layui-form-item .layui-form-select dl {
     max-height: 150px;
 }
 
 .layui-layer-content .layui-form .layui-form-item .button-box {
     padding-top: 20px;
 }
 
 .layui-layer-content p {
     color: #75757a;
     font-size: 14px;
     line-height: 30px;
     text-align: center;
 }
 
 .layui-layer-content .bottom {
     text-align: center;
     padding-top: 25px;
 }
 
 .layui-layer-content .btn-default {
     color: #fff;
     text-shadow: none;
     width: 120px;
     height: 36px;
     background-color: #e6614f;
     border: 1px solid #c74545;
     outline: 0;
     margin-right: 10px;
 }
 
 .layui-layer-content .btn-default:hover {
     color: #fff;
     text-shadow: none;
     background-color: #f5715f;
     border: 1px solid #c74545;
     box-shadow: none;
     outline: 0;
 }
 
 .layui-layer-content .btn-default:active:focus {
     color: #fff;
     background-color: #f5715f;
     border-color: transparent;
     border-top: 4px solid #c74545;
     box-shadow: none;
     outline: 0;
 }
 /* global style end */
 /* --------------------------------------------------
-------------------------------------------------- */
 /*notPass start*/
 
 .frame-content .status-box {
     padding: 20px 40px;
 }
 
 .frame-content .status-box .login {
     padding-top: 10px;
     padding-bottom: 30px;
 }

  .frame-content .reviewing .tips-box {
     padding-top: 10px;
     padding-bottom: 20px;
 }
 
 .frame-content .status-box p.tips {
     padding-left: 20px;
 }

 .frame-content .status-box p i {
     top: 7px;
     left: 0;
 }

 
 /*notPass end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*notPass start*/
 /*notPass end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*adPreview*/
 
 .layui-layer-content .ad-preview {
     max-height: 400px;
     overflow: hidden;
 }
 
 .layui-layer-content .ad-preview img {
     width: auto;
     max-height: 400px;
 }
 
 .carousel-caption {
     color: #555;
     padding-bottom: 15px;
 }
 
 .carousel-caption h2 {
     font-size: 16px;
 }
 
 .carousel-control {
     position: absolute;
     top: 0;
     bottom: 0;
     left: 0;
     width: 15%;
     font-size: 20px;
     color: #fff;
     text-align: center;
     text-shadow: 0 1px 2px rgba(0, 0, 0, .6);
     background-color: rgba(0, 0, 0, 0);
     filter: alpha(opacity=20);
     opacity: .1;
 }
 
 .carousel-control:focus,
 .carousel-control:hover {
     color: #fff;
     text-decoration: none;
     filter: alpha(opacity=30);
     outline: 0;
     opacity: .2;
 }
 /*adPreview end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*store-list*/
 
 .layui-layer-content .store-list ul {
     /*padding-top: 25px;*/
     max-height: 430px;
     overflow-y: scroll;
 }
 
 .layui-layer-content .store-list ul li {
     color: #2d2b2b;
     font-size: 12px;
     line-height: 24px;
 }
 /*store-list end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*pay-tips*/
 
 .layui-layer-content .pay-tips .top {
     border-bottom: 1px solid #d5d5d5;
     padding-bottom: 15px;
 }
 
 .layui-layer-content .pay-tips .top p span {
     color: #e6614f;
     font-weight: bold;
 }
 
 .layui-layer-content .pay-tips p.read {
     text-align: left;
     line-height: 24px;
     padding-top: 20px;
     /*letter-spacing: -1px;*/
 }
 
 .layui-layer-content .pay-tips .bottom {
     padding-bottom: 0;
 }
 
 .layui-layer-content .pay-tips .top p.warning,
 .layui-layer-content .result-tips .top p.warning {
     color: #e6614f;
     font-weight: bold;
     padding-bottom: 15px;
 }
 
 .layui-layer-content .pay-tips .top p.title,
 .layui-layer-content .result-tips .top p.warning {
     text-align: left;
 }
 
 .layui-layer-content .pay-tips .top p span a,
 .layui-layer-content .result-tips .top p span a {
     color: #e6614f;
     font-weight: normal;
 }
 
 .layui-layer-content .pay-tips-status .top,
 .layui-layer-content .result-tips .top {
     padding-top: 30px;
 }
 /*result-tips start*/
 
 .layui-layer-content .result-tips .top p {
     text-align: left;
 }
 
 .layui-layer-content .result-tips .top h2 {
     line-height: 30px;
 }
 
 .layui-layer-content .result-tips .top h2 span {
     font-weight: bold;
 }
 
 .layui-layer-content .pay-tips-status .bottom {
     padding-bottom: 20px;
 }
 /*result-tips end*/
 /*pay-tips end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*edit-Plan*/
 
 .layui-layer-content .edit-plan,
 .layui-layer-content .edit-group {
     /*padding: 20px 100px 40px 100px;*/
     /*text-align: center;*/
 }
 
 .layui-layer-content .edit-plan p,
 .layui-layer-content .edit-group p {
     text-align: left;
     margin-bottom: 10px;
 }
 
 .layui-layer-content .edit-plan input[type="text"],
 .layui-layer-content .edit-group input[type="text"] {
     /*color: #bababa;*/
     font-size: 12px;
     width: 360px;
     /*width: 100%;*/
     height: 40px;
     padding-left: 20px;
     border: 1px solid #d5d5d5;
     border-radius: 2px;
     margin-bottom: 25px;
 }
 
 .layui-layer-content .edit-plan input.warning,
 .layui-layer-content .edit-group input.warning {
     color: #e6614f;
     border-color: #c47773;
     box-shadow: none;
 }
 
 .layui-layer-content .edit-plan .bottom,
 .layui-layer-content .edit-group .bottom {
     padding: 0;
 }
 
 .layui-layer-content .edit-plan .btn-default,
 .layui-layer-content .edit-group .btn-default {
     width: 120px;
     height: 35px;
 }
 /*edit-Plan end*/
 /*editGroup end*/
 /* --------------------------------------------------
-------------------------------------------------- */
 /*submitReview*/
 
 .layui-layer-content .submit-review p {
     display: inline-block;
     width: 100%;
     color: #e6614f;
     font-size: 14px;
     text-align: center;
     line-height: 26px;
     font-weight: bold;
     /*margin-bottom: 25px;*/
 }
 
 .submit-review .btn-default {
     width: 120px;
     height: 36px;
 }
 /*submitReview end*/
