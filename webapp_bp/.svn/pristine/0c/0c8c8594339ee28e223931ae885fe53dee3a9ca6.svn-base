-------------------------------------------------- */
 /*adTextPic*/
 
 .layui-layer-content .ad-text-pic {
     min-width: 500px;
     background-color: #fff;
     /*border: 1px solid #d5d5d5;*/
 }
 
 .layui-layer-content .ad-text-pic img {
     width: 100%;
     max-height: 600px;
 }
 
 .layui-layer-content .ad-text-pic p {
     color: #666;
     font-size: 14px;
     text-align: center;
     line-height: 26px;
     border: none;
 }
