 /*invoiceCheck start*/
 
 .layui-layer-content .invoice-check {
     padding: 0 40px;
 }
 
 .layui-layer-content .invoice-check p.tips {
     color: #666;
     font-size: 12px;
     line-height: 26px;
     float: inherit;
 }
 
 .layui-layer-content .invoice-check p.tips span {
     color: #e6614f;
     font-size: 14px;
     font-weight: bold;
 }
 
 .layui-layer-content .invoice-check p.tips span.status-tips {
     color: #666;
     font-size: 12px;
     font-weight: normal;
 }
 
 .layui-layer-content .invoice-check .info {
     padding: 20px 0;
     margin: 20px 0;
     border-top: 1px solid #d5d5d5;
     border-bottom: 1px solid #d5d5d5;
 }
 
 .layui-layer-content .invoice-check .info p {
     color: #666;
     font-size: 14px;
     line-height: 28px;
     text-align: left;
 }
 
 .layui-layer-content .invoice-check .info p.title {
     color: #953232;
     font-size: 14px;
     line-height: 28px;
 }
 
 .layui-layer-content .invoice-check .info p span.cash {
     color: #e6614f;
     font-weight: bold;
 }
