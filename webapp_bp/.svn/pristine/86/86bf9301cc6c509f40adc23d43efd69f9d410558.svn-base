/*
 * Globals
 */

html,
body {
	height: 100%;
	background-color: #f8f8f8;
}

body {
	text-align: center;
	font-family: "微软雅黑";
}

.site-wrapper {
	display: table;
	width: 100%;
	height: 100%;
	min-height: 100%;
}

.site-wrapper-inner {
	display: table-cell;
	vertical-align: middle;
}


/*checkbox*/

.login label {
	padding-left: 0;
}

.checkbox span.checkbox {
	width: 14px;
	height: 14px;
	min-height: auto;
	line-height: 14px;
	display: inline-block;
	position: relative;
	margin-right: 6px;
	vertical-align: middle;
	margin-top: 1px;
}

.checkbox span.checkbox input {
	filter: alpha(opacity=0);
	-moz-opacity: 0;
	-khtml-opacity: 0;
	-webkit-opacity: 0;
	opacity: 0;
	width: 16px;
	height: 16px;
	position: relative;
	z-index: 9;
}

.checkbox span.checkbox i {
	width: 14px;
	height: 14px;
	border: 1px solid #979797;
	border-radius: 1px;
	display: inline-block;
	position: absolute;
	left: 0;
	top: 0;
}

.checkbox span.checkbox i {
	border-radius: 3px;
}

.checkbox span.checkbox input:checked+ i {
	background-color: #e6614f;
	border-color: #c74545;
}

.checkbox span.checkbox input:checked+ i:after {
	content: "";
	width: 6px;
	height: 4px;
	display: inline-block;
	position: absolute;
	left: 50%;
	top: 50%;
	border: 2px solid #fff;
	margin-left: -3px;
	margin-top: -3px;
	border-top: 0;
	border-right: 0;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	transform: rotate(-45deg);
}


/*checkbox*/


/*
 * Globals end
 */


/*
 * login-container
 */

.login-container {
	width: 850px;
	background-color: #fff;
	border-radius: 5px;
	margin-right: auto;
	margin-left: auto;
}

.login {
	padding: 0 20px;
}

.login .logo {
	padding-top: 85px;
	padding-bottom: 50px;
}

.login h2 {
	color: #664545;
	font-size: 26px;
	margin: 0;
	margin-bottom: 40px;
}

.login input[type="text"],
.login input[type="password"] {
	color: #bababa;
	font-size: 12px;
	height: 50px;
	padding-left: 45px;
	border: 1px solid #d5d5d5;
	border-radius: 2px;
}

.login input:focus {
	color: #e6614f;
	border-color: #c47773;
	box-shadow: none;
}

.login .form-group {
	position: relative;
	width: 360px;
	font-size: 12px;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 10px;
}

.login .form-group i {
	width: 20px;
	height: 20px;
	text-align: center;
	position: absolute;
	top: 15px;
	left: 12px;
}

.login .form-group i.user-icon {
	background: url(../img/css-sprite.png) 0 0 no-repeat;
}

.login .form-group i.password-icon {
	background: url(../img/css-sprite.png) -30px 0 no-repeat;
}

.login .form-group .verification-code {
	width: 220px;
	height: 50px;
	float: left;
}

.login .form-group .verification-code input {
	padding-left: 20px;
}

.login .form-group .verification-code-pic {
	float: right;
}

.login .form-group .verification-code-pic img {
	border-radius: 3px;
}

.login .form-group .checkbox {
	color: #757575;
	float: left;
	padding-top: 0;
}

.login .form-group .checkbox label input[type="checkbox"] {
	margin-top: 2px;
}

.login .form-group p.tips {
	position: relative;
	color: #f56760;
	font-size: 12px;
	float: right;
	padding-left: 20px;
}

.login .form-group p i {
	width: 14px;
	height: 14px;
	text-align: center;
	position: absolute;
	top: 1px;
	left: 0;
}

.login .form-group p i.exclamation-icon {
	background: url(../img/css-sprite.png) -100px 0 no-repeat;
}

.login .form-group p.account {
	color: #75757a;
}

.login .button-box {
	padding-bottom: 30px;
	margin-bottom: 0;
}

.btn-default {
	color: #fff;
	text-shadow: none;
	width: 100%;
	height: 50px;
	background-color: #e6614f;
	border: 1px solid #c74545;
	outline: 0;
}

.btn-default:hover,
.btn-default:focus {
	color: #fff;
	text-shadow: none;
	width: 100%;
	height: 50px;
	background-color: #f5715f;
	border: 1px solid #c74545;
	outline: 0;
}

.btn-default:active:focus {
	color: #fff;
	background-color: #f5715f;
	border-color: transparent;
	border-top: 5px solid #c74545;
	box-shadow: none;
	outline: 0;
}

.login .account-box {
	padding-bottom: 105px;
	margin-bottom: 0;
}

.login .account-box p {
	padding: 0;
	margin: 0;
}

.login .account-box p.account a.creat,
.login .account-box p.account a.creat:hover {
	color: #f56760;
	text-decoration: none;
	padding: 0 5px;
}

.login .account-box p.account a.forget,
.login .account-box p.account a.forget:hover {
	color: #75757a;
	text-decoration: none;
	padding: 0 5px;
}


/*
 * login-container end
 */


