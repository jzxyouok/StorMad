    <!--section-->
    <section class="ad account">
        <div class="container">
            <div class="search-box">
                <form class="layui-form">
                    <div class="layui-form-item">
                        <label class="layui-form-label">查询条件筛选：</label>
                        <div class="layui-input-inline">
                            <input name="yearMonth" class="layui-input btn-yearMonth" placeholder="选择年月" onclick="layui.laydate({format:'YYYY-MM',elem: this, festival: true})">
                        </div>
                        <div class="layui-input-inline btn-type">
                            <select name="type" id="type">
                                <option value="">充值类型</option>
                                <option value="">全部</option>
                                <option value="1">现金</option>
                                <option value="2">货款</option>
                            </select>
                        </div>
                        <div class="layui-input-inline">
                            <button class="btn btn-default search-btn" lay-submit lay-filter="serach" type="button"><i class="fa fa-search" aria-hidden="true"></i></button>
                        </div>
                        <div class="layui-input-inline output-log pull-right">
                            <p>该月充值总额：<span id="balance"></span></p>
                            <button class="btn btn-default" type="button">导出充值记录</button>
                        </div>
                    </div>
                </form>
            </div>
            <table class="table table-hover ad-list">
                <thead>
                    <tr>
                        <th>充值时间</th>
                        <th>充值类型</th>
                        <th>充值金额</th>
                    </tr>
                </thead>
                <tbody id="account_info" class="show_info">
                </tbody>
            </table>
            <!-- empty-tips start -->
            <div class="empty-tips no_info" style="display: none;">
                <p><i class="exclamation-icon" aria-hidden="true"></i><span>当前无充值记录。</span></p>
