            <div class="tips-box">
                <p><span>原因：</span><span class="reason"></span> </p>
            </div>
            <div class="button-box">
                <button type="submit" class="btn btn-default" id="btnAlter">马上修改信息</button>
            </div>
        </div>
    </div>
    <!-- notPass end -->
    <!-- notPass start -->
    <div id="reviewing" class="frame-content" style="display: none;">
        <div class="frame-body status-box reviewing">
            <div class="login">
                <!-- <div class="logo">
                    <img src="../img/logo_login.png" />
                </div> -->
                <h2>开户信息审核中(..•˘_˘•..)...</h2>
