<body>
    <!-- wrapper -->
    <div class="wrapper">
        <!-- wrapper-inner -->
        <div class="wrapper-inner">
            <!-- login-container -->
            <div class="wrapper-container login-container">
                <div class="logo">
                    <img src="../img/logo_login.png" />
                </div>
                <h2>用户登录</h2>
                <form class="form-horizontal" id="loginForm" role="form">
                    <div class="form-group">
                        <i class="user-icon"></i>
                        <input type="text" class="form-control" id="loginName" name="loginName" placeholder="输入用户名">
                    </div>
                    <div class="form-group">
                        <i class="password-icon"></i>
                        <input type="password" class="form-control" id="password" name="password" placeholder="输入密码">
                    </div>
                    <div class="form-group">
                        <div class="verification-code">
                            <input type="text" class="form-control" id="vcode" name="vcode" placeholder="输入验证码" maxlength="5">
                            <input type="hidden" id="vid" name="vid">
                        </div>
                        <div class="verification-code-pic">
                            <!-- <img id="code-pic" src="http://placehold.it/130x50"> -->
                            <img id="code-pic">
                        </div>
                    </div>
                    <div class="form-group">
                        <!-- <div class="checkbox-main">
                            <label>
                                <span><input name="" id="" type="checkbox"><i></i> </span> 记住密码
                            </label>
                        </div> -->
                        <p class="tips">
                            <!-- <i class="exclamation-icon"></i>密码有误 -->
                        </p>
                    </div>
                    <div class="form-group button-box">
                        <button type="button" id="btnSubmit" class="btn btn-default">登录</button>
                    </div>
                    <div class="form-group account-box">
                        <p class="account">
                            没有账号？
                            <a class="creat" href="../ader/login-new-account.html">马上开户</a> |
                            <a class="forget" href="../ader/login-forgot-password.html">忘记密码</a>
                        </p>
                    </div>
                </form>
            </div>
            <!-- /.login-container -->
            <!--footer-->
            <footer>
                <p>客服电话：0591-86290888
                    <br> Copyright @2016 永辉超市股份有限公司 All Rights Reserved.</p>
            </footer>
            <!--footer-->
