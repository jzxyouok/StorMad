/*header*/

header {
	height: 90px;
	background-color: #f8f8f8;
	border-bottom: 1px solid #d5d5d5;
	margin-bottom: 25px;
}

header .container {
	position: relative;
}

header h2 {
	display: inline-block;
	color: #664545;
	font-size: 26px;
	font-weight: bold;
	line-height: 90px;
	font-family: 微软雅黑;
}

header h2 span {
	color: #e6614f;
	font-size: 14px;
}

header p {
	display: inline-block;
	color: #664545;
	font-size: 14px;
	font-weight: bold;
	line-height: 80px;
	margin-top: 10px;
}

header p span {
	color: #e6614f;
}

header form {
	color: #997244;
	font-size: 14px;
	line-height: 90px;
}

header .button-box {
	display: inline-block;
}

header .button-box .btn-default {
	color: #997244;
	font-size: 12px;
	height: 36px;
	line-height: 36px;
	background-color: #e9c084;
	border-radius: 2px;
	border: 1px solid #b98f5d;
	outline: 0;
	padding: 0 20px;
}

header .button-box .btn-default:hover {
	color: #997244;
	text-shadow: none;
	background-color: #f7cc8e;
	border: 1px solid #b98f5d;
	box-shadow: none;
	outline: 0;
}

header .button-box .btn-default:active {
	color: #997244;
	background-color: #f7cc8e;
	border-color: transparent;
	border-top: 4px solid #b98f5d;
	box-shadow: none;
	outline: 0;
}


/*switch*/

.pull-right {
	line-height: 90px;
}

.pull-right h3 {
	color: #997244;
	font-size: 14px;
	display: inline-block;
	margin: 0;
}

.pull-right .layui-form {
	display: inline-block;
	position: relative;
}

.pull-right .layui-form .layui-form-item {
	width: 110px;
	height: 30px;
}

.pull-right .layui-form-item .layui-form-label {
	float: left;
	display: block;
	padding: 0;
	width: auto;
	color: #997244;
	font-size: 14px;
	font-weight: normal;
	text-align: right;
	line-height: 90px;
}

.pull-right .layui-form-item .layui-input-inline {
	float: left;
	width: auto;
	margin-right: 10px;
}

.pull-right .layui-form-switch {
	position: relative;
	height: 30px;
	line-height: 28px;
	width: 72px;
	padding: 0 5px;
	margin-top: 0px;
	border: 1px solid #d2d2d2;
	border-radius: 20px;
	background-color: #fff;
	transition: .1s linear;
}

.pull-right .layui-form-onswitch {
	border-color: #E6614F;
	background-color: #E6614F;
}

.layui-form-onswitch::before {
	content: '开启';
	color: #fff;
	padding-left: 6px;
}

.pull-right .layui-form-switch i {
	position: absolute;
	left: 5px;
	top: 3px;
	width: 22px;
	height: 22px;
	border-radius: 20px;
	background-color: #d2d2d2;
	-webkit-transition: .1s linear;
	transition: .1s linear;
}

.pull-right .layui-form-onswitch i {
	left: 44px;
	background-color: #fff;
}


