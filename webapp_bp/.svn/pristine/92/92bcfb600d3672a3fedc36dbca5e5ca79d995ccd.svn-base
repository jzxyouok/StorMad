/*auction-record*/

.auction-record {
	position: absolute;
	top: 49px;
	right: 0px;
	width: 385px;
	transition: .2s;
}

.auction-record .record-show {
	position: absolute;
	top: 42px;
	/*left: 0px;*/
	left: 348px;
	color: #997244;
	font-size: 12px;
	width: 36px;
	height: 136px;
	border-radius: 2px;
	border: 1px solid #b98f5d;
	background-color: #f7cc8e;
	cursor: default;
}

