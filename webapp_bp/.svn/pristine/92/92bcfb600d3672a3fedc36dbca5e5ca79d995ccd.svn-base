/*switch*/

.remind-window {
	position: absolute;
	top: 70px;
	right: 0;
	text-align: center;
	width: 350px;
	padding: 30px;
	background-color: #fff;
	border-radius: 2px;
	border: 1px solid #d5d5d5;
	z-index: 999;
}

.remind-window .remind-box p {
	width: 100%;
	color: #75757a;
	font-size: 12px;
	line-height: 24px;
	font-weight: normal;
	text-align: left;
	padding-bottom: 12px;
	border-bottom: 1px solid #d5d5d5;
}

.remind-window .remind-box p.remind-content {
	color: #e6614f;
	margin-bottom: 30px;
}

.remind-window .remind-box .btn-default {
	color: #fff;
	text-shadow: none;
	width: 120px;
	height: 36px;
	background-color: #e6614f;
	border: 1px solid #c74545;
	outline: 0;
}

.remind-window .remind-box .btn-default:hover {
	color: #fff;
	text-shadow: none;
	background-color: #f5715f;
	border: 1px solid #c74545;
	box-shadow: none;
	outline: 0;
}

.remind-window .remind-box .btn-default:active {
	color: #fff;
	background-color: #f5715f;
	border-color: transparent;
	border-top: 4px solid #c74545;
	box-shadow: none;
	outline: 0;
}


/*header end*/


/*section*/

section {
	position: relative;
	padding-bottom: 85px;
}

section .container {
	position: relative;
}


/*auction*/


/*nav-tabs*/

.auction .nav-tabs {
	background: url("../img/nav-tabs-bg.png") 0px 0px no-repeat;
}

.auction .nav> li> a {
	position: relative;
	display: block;
	width: 250px;
	height: 50px;
	text-align: center;
	padding: 0;
	line-height: 50px !important;
}

.nav-tabs> li> a,
.nav-tabs> li> a:focus,
.nav-tabs> li> a:hover {
	color: #969595;
	cursor: default;
	background-color: transparent;
	border: none;
	border-bottom-color: transparent;
}

.nav-tabs> li.active> a,
.nav-tabs> li.active> a:focus,
.nav-tabs> li.active> a:hover {
	color: #666;
	cursor: default;
	background-color: transparent;
	border: none;
	border-bottom-color: transparent;
}

.auction .nav-tabs> li> a i {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 17px;
	left: 75px;
}

.auction .nav-tabs> li.active> a i.auction-icon-01 {
	background: url(../img/css-sprite.png) -180px 0px no-repeat;
}

.auction .nav-tabs> li> a i.auction-icon-01 {
	background: url(../img/css-sprite.png) -180px -16px no-repeat;
}

.auction .nav-tabs> li.active> a i.auction-icon-02 {
	background: url(../img/css-sprite.png) -205px 0px no-repeat;
}

.auction .nav-tabs> li> a i.auction-icon-02 {
	background: url(../img/css-sprite.png) -205px -16px no-repeat;
}

.auction .nav-tabs> li.active> a i.auction-icon-03 {
	background: url(../img/css-sprite.png) -230px 0px no-repeat;
}

.auction .nav-tabs> li> a i.auction-icon-03 {
	background: url(../img/css-sprite.png) -230px -16px no-repeat;
}


/*nav-tabs end*/


/*tab-content*/


/*tabpanel 01*/

.search-box {
	/*position: relative;*/
	height: 70px;
	line-height: 70px;
}

.search-box label {
	/*position: absolute;*/
	display: inline-block;
	right: 260px;
	color: #666;
	font-size: 12px;
	font-weight: normal;
}

.search-box .form-control {
	display: inline-block;
	width: 250px;
	height: 40px;
	border: none;
	border-radius: 0;
	padding-right: 45px;
	margin-top: 15px;
	background: url(../img/css-sprite.png) 0px -60px no-repeat;
}

.search-box .form-control:focus,
.search-box .form-control:hover {
	background: url(../img/css-sprite.png) 0px -100px no-repeat;
	border: none;
	box-shadow: none;
}

.table-responsive {
	border-top: 1px solid #d5d5d5;
}

.table-hover> tbody> tr.active:hover td,
.table-hover> tbody> tr.active td,
.table-hover> tbody> tr:hover td {
	/*font-weight: bold;*/
	background-color: #f8f8f8;
	border-bottom: 1px solid #d5d5d5;
}

.table> thead> tr> th {
	vertical-align: middle;
	border-bottom: 1px solid #d5d5d5;
}

.table> thead> tr> th {
	color: #953232;
	font-size: 12px;
	font-weight: normal;
	padding: 0px 8px;
	line-height: 50px;
}

.table> tbody> tr> td {
	color: #666;
	font-size: 12px;
	padding: 0px 8px;
	line-height: 60px;
}


/*tabpanel 01 td checked*/

.table> tbody> tr> td span.checked {
	width: 18px;
	height: 18px;
}

.table> tbody> tr> td span.checked i {
	position: absolute;
	width: 18px;
	height: 18px;
	border: 2px solid #dee0e6;
	border-radius: 50%;
	margin-top: 20px;
}

.table> tbody> tr:hover> td span.checked i,
.table> tbody> tr.active> td span.checked i {
	border: 5px solid #e6614f;
	transition: .2s;
	-moz-transition: .2s;
	/* Firefox 4 */
	-webkit-transition: .2s;
	/* Safari 和 Chrome */
	-o-transition: .2s;
	/* Opera */
}


/*tabpanel 01 td checked end*/

.table> tbody> tr.after> td {
	color: #bababa;
}

.table> tbody> tr> td span.status-tips {
	position: relative;
	color: #e6614f;
	padding-left: 20px;
}

.table> tbody> tr> td span.status-tips i {
	position: absolute;
	top: -1px;
	left: 0;
	width: 14px;
	height: 14px;
}

.table> tbody> tr> td span.status-tips i.tips-icon {
	background: url(../img/css-sprite.png) -100px -20px no-repeat;
}

.table> tbody> tr> td span.status-tips a {
	color: #e6614f;
}

.table> tbody> tr> td {
	border-top: 1px solid #d5d5d5;
}

.table> tbody> tr:last-child {
	border-bottom: 1px solid #d5d5d5;
}


/*extend*/

.table-hover> tbody> tr.extend td,
.table-hover> tbody> tr.extend:hover td {
	font-weight: normal;
	background-color: #f8f8f8;
}

.table> tbody> tr.extend> td {
	text-align: left;
	padding: 30px 38px;
	line-height: 1;
}

.extend .row .choice-title {
	text-align: right;
	line-height: 30px;
	padding-right: 5px;
}

.extend .row .industry {
	padding-left: 5px;
	padding-right: 5px;
	border-right: 2px solid #c54f3f;
}

.extend .row .industry button {
	display: inline-block;
	color: #666;
	font-size: 12px;
	height: 30px;
	line-height: 30px;
	padding: 0 18px;
	border: 2px solid #e6614f;
	border-radius: 15px;
	background-color: #fff;
	margin-right: 10px;
	margin-bottom: 10px;
}

.extend .row .industry button.active,
.extend .row .industry button:hover,
.extend .row .industry button:focus {
	color: #fff;
	background-color: #e6614f;
}

.extend .row .perfunctory {
	padding-left: 40px;
}

.extend .row .perfunctory p {
	color: #666;
	line-height: 30px;
	margin-bottom: 10px;
}

.extend .row .perfunctory p span {
	color: #e6614f;
}

.extend .row .perfunctory p .fa {
	font-size: 24px;
	color: #ffbf4f;
}

.extend .row .perfunctory p .disabled {
	color: #e7e7e7;
}

.extend .row .perfunctory .btn-default {
	color: #fff;
	text-shadow: none;
	width: 110px;
	height: 36px;
	background-color: #e6614f;
	border: 1px solid #c74545;
	outline: 0;
}

.extend .row .perfunctory .btn-default:hover {
	color: #fff;
	text-shadow: none;
	background-color: #f5715f;
	border: 1px solid #c74545;
	box-shadow: none;
	outline: 0;
}

.extend .row .perfunctory .btn-default:active {
	color: #fff;
	background-color: #f5715f;
	border-color: transparent;
	border-top: 4px solid #c74545;
	box-shadow: none;
	outline: 0;
}


/*extend end*/


